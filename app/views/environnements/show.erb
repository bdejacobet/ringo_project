<h2 class="title"><%= @environnement.name %></h2>
<div class="inner">
  <%= @environnement.description %>
</div>
<% @environnement.branches.each do |branch| %>
<div class="inner">
  <table class="table">
	<tr>
	  <th class="first"><%= branch.name %></th>
	  <th><%= branch.description %></th>
	  <th class="last"><%= button_to 'voir', url(:branches, :show, :id => branch.id), :method => :get, :class => :button_to %></th>
	</tr>		
  </table>
</div>
<% end %>
<div class="inner">
	<%= button_to 'retour sur le projet', url(:projets, :show, :id => @environnement.projet_id), :method => :get, :class => :button_to %>
</div>
